{
  "name": "navo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p .",
    "postbuild": "copyfiles -u 2 \"navo/web/**/*\" dist/web",
    "demo": "npm run build && node dist/navo/demo.js",
    "start": "node dist/navo/start.js",
    "dev": "tsx watch navo/start.ts",
    "dev:server": "nodemon --exec tsx navo/start.ts --watch navo/",
    "dev:web": "tsx watch navo/server.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "engines": {
    "node": "22.x"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^24.2.1",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.0",
    "prettier": "^3.6.2",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.40.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.15.0",
    "@vercel/speed-insights": "^1.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "pg": "^8.16.3"
  }
}
