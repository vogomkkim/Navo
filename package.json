{
  "name": "navo-monorepo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.18.3",
  "scripts": {
    "build": "pnpm -r build",
    "lint": "pnpm -r --if-present lint",
    "format": "biome format --write .",
    "db:generate": "pnpm exec dotenv -e .env -- pnpm exec drizzle-kit generate",
    "db:push": "pnpm exec dotenv -e .env -- pnpm exec drizzle-kit push",
    "db:pull": "pnpm exec drizzle-kit introspect",
    "clean": "pnpm -r clean && Remove-Item -Recurse -Force node_modules"
  },
  "engines": {
    "node": ">=20.11"
  },
  "dependencies": {
    "esbuild": "^0.25.9"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@next/eslint-plugin-next": "^15.5.2",
    "@types/node": "^24.2.1",
    "cross-env": "^10.0.0",
    "dotenv-cli": "^10.0.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.33.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "execa": "^9.6.0",
    "json-schema-to-zod": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vitest": "^3.2.4",
    "zod": "^3.24.2"
  }
}
