{
  "name": "@navo/server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "lint": "eslint . --ext .ts --config ../config/eslint.config.js",
    "test": "jest --config jest.config.ts",
    "db:generate": "drizzle-kit generate --config ../config/drizzle.config.ts",
    "db:push": "drizzle-kit push --config ../config/drizzle.config.ts",
    "db:pull": "drizzle-kit introspect --config ../config/drizzle.config.ts"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.49.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/static": "^8.2.0",
    "@google/generative-ai": "^0.15.0",
    "@types/uuid": "^10.0.0",
    "cors": "^2.8.5",
    "dompurify": "^3.0.6",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.5",
    "fastify": "^5.5.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "pino": "^9.9.0",
    "postgres": "3.4.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^24.2.1",
    "@types/pg": "^8.15.5",
    "copyfiles": "^2.4.1",
    "cross-env": "^10.0.0",
    "dotenv-cli": "^10.0.0",
    "drizzle-kit": "^0.31.4",
    "jest": "^30.1.2",
    "nodemon": "^3.1.0",
    "pino-pretty": "^13.1.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.5.4"
  }
}
