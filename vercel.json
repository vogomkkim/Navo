{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "navo/server.js": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "navo/server.js"
    },
    {
      "src": "/",
      "dest": "web/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "web/$1"
    }
  ]
}